<% if(typeof errors != 'undefined'){ %> <% errors.forEach(function(error) { %>
<div class="form-group alert alert-warning alert-dismissible show" role="alert">
  <%= error.msg %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% }); %> <% } %>

 <% if(success_msg != ''){ %>
<div class="form-group alert alert-success alert-dismissible show" role="alert">
  <%= success_msg %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>

<% if(error_msg != ''){ %>
<div class="form-group alert alert-danger alert-dismissible show" role="alert">
  <%= error_msg %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>

<% if(error != ''){ %>
<div class="form-group alert alert-danger alert-dismissible show" role="alert">
  <%= error %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>
